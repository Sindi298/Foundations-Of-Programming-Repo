#include <stdio.h>

struct student
{
    int studentno;
    char name;
    float marks;
};

void main()
{
    struct student s1 = {1, "Sindi", 92.6}
    struct student s2 = {2, "Jenny", 67.3};

    printf("Info for s1\n");
    printf("%d %s %f",s1.studentno, s1.name, s1.marks);
    printf("Info for s2\n");
    printf("%d %s %f",s2.studentno, s2.name, s2.marks);
}